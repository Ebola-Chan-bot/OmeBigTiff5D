	"OBT5.OmeBigTiff5D.Create": {
		"inputs": [
			{
				"name": "FilePath",
				"kind": "required",
				"type": [
					[
						"file"
					],
					[
						"string",
						"scalar"
					],
					[
						"char",
						"vector"
					]
				],
				"purpose": "文件路径"
			},
			{
				"name": "CreationDisposition",
				"kind": "required",
				"type": "OBT5.CreationDisposition",
				"purpose": "如果指定的文件存在或不存在，应该执行何种操作？"
			},
			{
				"mutuallyExclusiveGroup": [
					[
						{
							"name": "ImageDescription",
							"kind": "ordered",
							"type": [
								[
									"char",
									"vector"
								],
								[
									"string",
									"scalar"
								]
							],
							"purpose": "OME XML 图像描述"
						}
					],
					[
						{
							"name": "SizeX",
							"kind": "namevalue",
							"type": [
								"numeric",
								"scalar"
							],
							"purpose": "图像X尺寸",
							"default": 1
						},
						{
							"name": "SizeY",
							"kind": "namevalue",
							"type": [
								"numeric",
								"scalar"
							],
							"purpose": "图像Y尺寸",
							"default": 1
						},
						{
							"name": "SizeC",
							"kind": "namevalue",
							"type": [
								"numeric",
								"scalar"
							],
							"purpose": "图像C尺寸",
							"default": 1
						},
						{
							"name": "SizeZ",
							"kind": "namevalue",
							"type": [
								"numeric",
								"scalar"
							],
							"purpose": "图像Z尺寸",
							"default": 1
						},
						{
							"name": "SizeT",
							"kind": "namevalue",
							"type": [
								"numeric",
								"scalar"
							],
							"purpose": "图像T尺寸",
							"default": 1
						},
						{
							"name": "DimensionOrder",
							"kind": "namevalue",
							"type": [
								"OBT5.DimensionOrder",
								"scalar"
							],
							"purpose": "图像维度顺序",
							"default": "OBT5.DimensionOrder.XYCZT"
						},
						{
							"name": "PixelType",
							"kind": "namevalue",
							"type": [
								"OBT5.PixelType",
								"scalar"
							],
							"purpose": "图像像素类型",
							"default": "OBT5.PixelType.UINT16"
						},
						{
							"name":"ChannelColors",
							"kind":"namevalue",
							"type":["uint32","vector"],
							"purpose":"各通道颜色，每个通道用一个uint32表示颜色"
						}
					]
				]
			}
		]
	},
